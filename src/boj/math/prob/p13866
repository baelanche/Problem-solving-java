import java.util.*;

public class p13866 {
    public static void main(String args[]) {
        Scanner sc = new Scanner(System.in);
        
        int[] a = {sc.nextInt(), sc.nextInt(), sc.nextInt(), sc.nextInt()};
        Arrays.sort(a);
        System.out.println(Math.abs(a[0] + a[3] - a[1] - a[2]));
    }
}
